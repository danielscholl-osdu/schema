              },
              "MakeUpTorqueMax": {
                "type": "number",
                "title": "Make Up Torque Maximum",
                "description": "Maximum Make Up Torque",
                "example": "6490 ft.lbf",
                "x-osdu-frame-of-reference": "UOM:moment of force"
