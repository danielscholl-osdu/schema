{
    "$id": "https://schema.opengroup.com/json/swps/work-product/WoodchuckWorkProductComponent/1",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "WoodchuckWorkProductComponent",
    "description": "the resulting work product components of a distinct round of (theoretical) woodchucking work activity, assuming that said woodchuck could indeed chuck wood",
    "type": "object",
    "properties": {
      "ResourceTypeID": {
        "description": "The SRN of the resource's resource type.",
        "type": "string",
        "pattern": "srn:type:work-product/WoodchuckWorkProductComponent:[0-9]*$"
      },
      "ResourceID": {
        "description": "The SRN which identifies this OSDU resource object at the version level.",
        "type": "string",
        "pattern": "srn:work-product/WoodchuckWorkProductComponent:[^/:]+:[0-9]+$"
      },
      "ResourceHomeRegionID": {
        "description": "The name of the home [cloud environment] region for this OSDU resource object.",
        "type": "string",
        "pattern": "srn:reference-data/OSDURegion:[A-Za-z0-9-]+:[0-9]*$"
      },
      "ResourceHostRegionIDs": {
        "description": "The name of the host [cloud environment] region(s) for this OSDU resource object.",
        "type": "array",
        "items": {
          "type": "string",
          "pattern": "srn:reference-data/OSDURegion:[A-Za-z0-9-]+:[0-9]*$"
        }
      },  
      "ResourceObjectCreationDateTime": {
        "description": "Timestamp of the time at which Version 1 of this OSDU resource object was originated.",
        "type": "string",
        "format": "date-time"
      },
      "ResourceVersionCreationDateTime": {
        "description": "Timestamp of the time when the current version of this resource entered the OSDU.",
        "type": "string",
        "format": "date-time"
      },
      "ResourceCurationStatus": {
        "description": "Describes the current Curation status. Possible values - CREATED, CURATING, CURATED.",
        "type": "string",
        "pattern": "srn:reference-data/ResourceCurationStatus:[^/:]+:[0-9]*$"
      },
      "ResourceLifecycleStatus": {
        "description": "Describes the current Resource Lifecycle status. Possible values - LOADING, RECEIVED, ACCEPTED, RESCINDED, DELETED,",
        "type": "string",
        "pattern": "srn:reference-data/ResourceLifecycleStatus:[^/:]+:[0-9]*$"
      },
      "ResourceSecurityClassification": {
        "description": "Classifies the security level of the resources. Possible values = RESTRICTED, CLASSIFIED, CONFIDENTIAL, MOST-CONFIDENTIAL  ",
        "type": "string",
        "pattern": "srn:reference-data/ResourceSecurityClassification:[^/:]+:[0-9]*$"
      },    
        "Data": 
        {
            "type": "object",
            "properties":
            {
                "GroupTypeProperties":
                {
                    "type": "object",
                    "properties": 
                    {
                        "Files": 
                        {
                           "type": "array",
                           "items": 
                           {
                                "description": "The SRN which identifies this OSDU File resource.",
                                "type": "string",
                                "pattern": "srn:file/[A-Za-z0-9-]+:[^/:]+:[0-9]*$"                    
                            }
                        },
                        "Artefacts":
                        {
                            "type": "array",
                            "items": 
                            {
                                "type": "object",
                                "properties":
                                {
                                    "RoleID": 
                                    {
                                        "description": "The SRN of this artefact's role.",
                                        "type": "string",
                                        "pattern": "srn:reference-data/ArtefactRole:[^/:]+:[0-9]*$"
                                    },
                                    "ResourceID": 
                                    {
                                        "description": "The SRN which identifies this OSDU Artefact resource.",
                                        "type": "string",
                                        "pattern": "srn:file/[A-Za-z0-9-]+:[^/:]+:[0-9]*$"
                                    }
                                }
                            }
                        }
                    }
                },
                "IndividualTypeProperties":
                {
                    "type": "object",
                    "properties": 
                    {
                        "allOf": [
                            {
                                "$ref": "https://schema.opengroup.com/json/smds/reference-data/AbstractWormProductComponent/1"
                            },
                            {
                                "Properties":
                                {
                    "WoodchuckingCompanyName": {
                      "type": "string",
                      "description": "Name of the woodchucking Company",
                      "pattern": "srn:master-data/Organisation:[^/:]+:[0-9]*$"
                    },
                    "WellboreID": {
                      "type": "string",
                      "description": "Wellbore ID",
                      "pattern": "srn:master-data/Wellbore:[^/:]+:[0-9]*$"
                    },
                    "QuantityOfWoodChucked": {
                      "type": "number",
                      "description": "Quantity of wood chucked for the duration of the woodchucking activity, assuming that wood was chucked as a result of the woodchucker being able to chuck wood"
                    },
                    "QuantityOfWoodChuckedUnitID": {
                      "type": "string",
                      "description": "Unit of Measure of the quantity of wood chucked.",
                      "pattern": "srn:reference-data/UnitOfMeasure:[^/:]+:[0-9]*$"
                    },
                    "WoodchuckingMethodType": {
                      "type": "string",
                      "description": "Method of woodchucking used",
                      "pattern": "srn:reference-data/WoodchuckingMethodType:[^/:]+:[0-9]*$"
                    },
                    "CorrectionType": {
                      "type": "string",
                      "description": "Information or type of corrections done to account for the possibility of the woodchucker not having been able to chuck wood this woodchucking activity period."
                    },
					"CanChuckWood": {
                    "description": "Indicates whether or not the woodchuck used for the woodchucking activity in question is in fact able to chuck wood. A value of Y indicates that the woodchuck could chuck wood.",
                    "type": "boolean"
					}
                  }
                }
                ]
                }
                },
                "ExtensionProperties":
                {
                    "type": "object",
                    "properties":
                    {
                    }
                }
            }
        }
    },
    "required": 
    [
        "ResourceID",
        "ResourceTypeID",
        "ResourceHomeRegionID",
        "ResourceHostRegionIDs",
        "ResourceObjectCreationDateTime",
        "ResourceVersionCreationDateTime",
        "ResourceCurationStatus",
        "ResourceLifecycleStatus",
        "ResourceSecurityClassification"
    ],
    "additionalProperties": false
}

