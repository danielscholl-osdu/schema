{
  "$id": "https://schema.osdu.opengroup.org/json/swps/work-product/WellboreTrajectoryWorkProductComponent/1",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "WellboreTrajectoryWorkProductComponent",
  "description": "",
  "type": "object",
  "properties": {
    "ResourceTypeID": {
      "description": "The SRN of the resource's resource type.",
      "type": "string",
      "pattern": "srn:type:work-product-component/WellboreTrajectory:[0-9]+$"
    },
    "ResourceID": {
      "description": "The SRN which identifies this OSDU resource object at the version level.",
      "type": "string",
      "pattern": "srn:work-product-component/WellboreTrajectory:[^/:]+:[0-9]+$"
    },
    "ResourceHomeRegionID": {
      "description": "The name of the home [cloud environment] region for this OSDU resource object.",
      "type": "string",
      "pattern": "srn:reference-data/OSDURegion:[A-Za-z0-9-]+:[0-9]*$"
    },
    "ResourceHostRegionIDs": {
      "description": "The name of the host [cloud environment] region(s) for this OSDU resource object.",
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "srn:reference-data/OSDURegion:[A-Za-z0-9-]+:[0-9]*$"
      }
    },
    "ResourceObjectCreationDateTime": {
      "description": "Timestamp of the time at which Version 1 of this OSDU resource object was originated.",
      "type": "string",
      "format": "date-time"
    },
    "ResourceVersionCreationDateTime": {
      "description": "Timestamp of the time when the current version of this resource entered the OSDU.",
      "type": "string",
      "format": "date-time"
    },
    "ResourceCurationStatus": {
      "description": "Describes the current Curation status. Possible values - CREATED, CURATING, CURATED.",
      "type": "string",
      "pattern": "srn:reference-data/ResourceCurationStatus:[^/:]+:[0-9]*$"
    },
    "ResourceLifecycleStatus": {
      "description": "Describes the current Resource Lifecycle status. Possible values - LOADING, RECEIVED, ACCEPTED, RESCINDED, DELETED,",
      "type": "string",
      "pattern": "srn:reference-data/ResourceLifecycleStatus:[^/:]+:[0-9]*$"
    },
    "ResourceSecurityClassification": {
      "description": "Classifies the security level of the resources. Possible values = RESTRICTED, CLASSIFIED, CONFIDENTIAL, MOST-CONFIDENTIAL  ",
      "type": "string",
      "pattern": "srn:reference-data/ResourceSecurityClassification:[^/:]+:[0-9]*$"
    },
    "Data": {
      "type": "object",
      "properties": {
        "GroupTypeProperties": {
          "type": "object",
          "properties": {
            "Files": {
              "type": "array",
              "items": {
                "description": "The SRN which identifies this OSDU File resource.",
                "type": "string",
                "pattern": "srn:file/[A-Za-z0-9-]+:[^/:]+:[0-9]*$"
              }
            },
            "Artefacts": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "RoleID": {
                    "description": "The SRN of this artefact's role.",
                    "type": "string",
                    "pattern": "srn:reference-data/ArtefactRole:[^/:]+:[0-9]*$"
                  },
                  "ResourceID": {
                    "description": "The SRN which identifies this OSDU Artefact resource.",
                    "type": "string",
                    "pattern": "srn:file/[A-Za-z0-9-]+:[^/:]+:[0-9]*$"
                  }
                }
              }
            }
          }
        },
        "IndividualTypeProperties": {
          "allOf": [
            {
              "$ref": "https://schema.osdu.opengroup.org/json/swps/reference-data/AbstractWorkProductComponent/1"
            },
            {
              "type": "object",
              "properties": {
                "ServiceCompanyID": {
                  "type": "string",
                  "description": "Name of the Survey Company",
                  "pattern": "srn:master-data/Organisation:[^/:]+:[0-9]*$"
                },
                "WellboreID": {
                  "type": "string",
                  "description": "Wellbore ID",
                  "pattern": "srn:master-data/Wellbore:[^/:]+:[0-9]*$"
                },
                "TopDepthMD": {
                  "type": "number",
                  "description": "Top Measured Depth"
                },
                "TopDepthMDUnitID": {
                  "type": "string",
                  "description": "Unit of Measure of the Top Measured Depth",
                  "pattern": "srn:reference-data/UnitOfMeasure:[^:]+:[0-9]*$"
                },
                "AzimuthReferenceType": {
                  "type": "string",
                  "description": "Azimuth Reference Type",
                  "pattern": "srn:reference-data/AzimuthReferenceType:[^/:]+:[0-9]*$"
                },
                "CalculationMethodType": {
                  "type": "string",
                  "description": "Calculation Method Type",
                  "pattern": "srn:reference-data/CalculationMethodType:[^/:]+:[0-9]*$"
                },
                "CorrectionType": {
                  "type": "string",
                  "description": "Information or type of corrections done for the trajectory data, like magnetic interference etc."
                },
                "SourceType/SurveyTool": {
                  "type": "string",
                  "description": "Use case: Search for surveys taken post-drilling operations (example Gyro), or during drilling (MWD)"
                },
                "MapProjection": {
                  "type": "string",
                  "description": "When AzimuthReferenceType is Grid, capture the map projection used to create that grid"
                },
                "Declination": {
                  "type": "string",
                  "description": "When AzimuthReferenceType is Grid, need to know the declination angle at that point on the globe."
                }
              }
            }
          ]
        },
        "ExtensionProperties": {
          "type": "object",
          "properties": {}
        }
